# JWT Configuration
JWT_SECRET=your_jwt_secret_here

# API Configuration
VITE_API_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_TOKEN_NAME=eduforum_token
VITE_TOKEN_EXPIRY_REDIRECT=true

# OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
SESSION_SECRET=your_session_secret_here
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/auth/google/callback

# Description of Variables:
# JWT_SECRET: Secret key for JWT token generation and verification
# VITE_API_URL: Base URL for API endpoints
# VITE_API_TIMEOUT: API request timeout in milliseconds
# VITE_AUTH_TOKEN_NAME: Name of the authentication token in localStorage
# VITE_TOKEN_EXPIRY_REDIRECT: Whether to redirect on token expiry
# VITE_GOOGLE_CLIENT_ID: Google OAuth client ID
# SESSION_SECRET: Secret key for session management
# VITE_GOOGLE_REDIRECT_URI: OAuth callback URL for Google authentication 